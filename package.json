{
  "name": "react-vite-template",
  "version": "0.1.0",
  "description": "Starter using Vite, React, Tailwind CSS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuab/react-vite-template.git"
  },
  "author": "St√©phane Clairbaux <stephane@clairbaux.com>",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/xuab/react-vite-template/issues"
  },
  "homepage": "https://github.com/xuab/react-vite-template#readme",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode mock",
    "build": "vite build",
    "build:mock": "vite build --mode mock",
    "preview": "vite preview --port 3000",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:e2e": "npm run build:mock && playwright test",
    "test:e2e:ci": "playwright test",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --noEmit --incremental --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/.eslintcache",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf ./node_modules/.cache ./build ./coverage"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "dependencies": {
    "immer": "^9.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "msw": "^0.38.2",
    "postcss": "^8.4.7",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
